{
  "functions": {
    "api/**/*.ts": {
      "runtime": "@vercel/node@5.3.5"
    }
  },
  "routes": [
    { "src": "/api/(.*)", "dest": "/api/$1.ts" },
    { "src": "/_next/static/(.*)", "dest": "/.next/static/$1" },
    { "src": "/(.*)", "dest": "/.next/$1" },
    { "src": "/(.*)", "dest": "/.next/index.html" }
  ]
}
